{
  "name": "ritcher-map-v2",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "pnpm run --parallel dev",
    "build": "pnpm run --parallel build",
    "prisma:studio": "pnpm --filter server prisma:studio",
    "prisma:generate": "pnpm --filter server prisma:generate",
    "migrate:dev": "pnpm --filter server migrate:dev",
    "migrate:reset": "pnpm --filter server migrate:reset",
    "seed": "cd apps/ui && pnpx prisma db seed",
    "start:migrate:dev": "pnpm --filter ui migrate:dev && pnpm --filter server dev",
    "ui:build": "pnpm --filter ui build",
    "ui:prod": "pnpm --filter ui start",
    "postinstall": "pnpm --filter ui prisma:gen"
  },
  "dependencies": {
    "@apollo/server": "^4.7.5",
    "@prisma/client": "^5.20.0",
    "class-validator": "0.13.2",
    "graphql": "16.7.1",
    "reflect-metadata": "0.1.13"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.70",
    "@types/chance": "1.1.3",
    "@types/node": "^20.3.1",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "chance": "1.1.11",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "2.8.8",
    "prisma": "5.20.0",
    "prisma-dbml-generator": "0.11.0-dev.0",
    "ts-loader": "9.4.4",
    "tsconfig-paths": "4.2.0"
  },
  "packageManager": "pnpm@9.0.4+sha512.c374b52de3de88c58fd8333df664a737279cdb0e1344ba4054d3b1fffa9a1a3670854f755dca4f16adea3f14be9896a7fcaf167409fe0c1ad60475271dafe81a"
}