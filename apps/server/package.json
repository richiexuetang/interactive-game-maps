{
  "name": "server",
  "version": "0.0.1",
  "scripts": {
    "pm2:deploy:app": "pnpm run build && pm2 start app.json",
    "pm2:start:app": "pm2 start app.json",
    "pm2:stop:app": "pm2 stop app.json",
    "pm2:destroy:app": "pm2 delete app.json",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "migrate:dev": "prisma migrate dev --preview-feature",
    "migrate:dev:create": "prisma migrate dev --create-only --preview-feature",
    "migrate:reset": "prisma migrate reset --preview-feature",
    "migrate:deploy": "pnpm exec prisma migrate deploy --preview-feature",
    "migrate:status": "pnpm exec prisma migrate status --preview-feature",
    "migrate:resolve": "pnpm exec prisma migrate resolve --preview-feature",
    "prisma:studio": "pnpm exec prisma studio",
    "prisma:generate": "npx prisma generate",
    "prisma:generate:watch": "pnpm exec prisma generate --watch",
    "start:db": "pnpm run migrate:deploy && pnpm run prisma:generate && pnpm run seed",
    "seed": "pnpm exec prisma db seed",
    "postinstall": "npm run prisma:generate",
    "docker:migrate": "docker-compose -f docker-compose.migrate.yml up -d",
    "docker:db": "docker-compose -f docker-compose.db.yml up -d",
    "docker:seed": "docker exec -it ritcher-map-api pnpm run seed",
    "docker": "docker-compose up -d",
    "docker:build": "docker-compose build"
  },
  "dependencies": {
    "@apollo/server": "^4.7.5",
    "@nestjs/apollo": "12.0.7",
    "@nestjs/common": "10.1.0",
    "@nestjs/config": "3.0.0",
    "@nestjs/core": "10.1.0",
    "@nestjs/graphql": "12.0.8",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^10.0.3",
    "@prisma/client": "^5.20.0",
    "class-transformer": "^0.5.1",
    "class-validator": "0.13.2",
    "cookie-parser": "^1.4.6",
    "express": "^4.21.0",
    "graphql": "16.7.1",
    "graphql-scalars": "^1.23.0",
    "graphql-subscriptions": "^2.0.0",
    "nestjs-prisma": "0.22.0-dev.0",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "0.1.13"
  },
  "devDependencies": {
    "@nestjs/cli": "10.1.10",
    "@nestjs/platform-express": "^10.4.4",
    "@nestjs/schematics": "10.0.1",
    "@nestjs/testing": "^10.4.4",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.70",
    "@types/express": "4.17.17",
    "@types/node": "^20.3.1",
    "prisma": "5.20.0",
    "prisma-dbml-generator": "0.12.0",
    "ts-loader": "9.4.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.5.2"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}